<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Temáticas de interés por programa curricular</ReportTitle><ReportDescription>Muestra los trabajos de grado, de acuerdo a un proyecto curricular seleccionado</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>show</gridDisplay><gridSortable>yes</gridSortable><gridSearchable>yes</gridSearchable><gridPageable>yes</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>select row_number() over (order by estado, numero) as num, numero, titulo, autores, estado, tematica
from (

SELECT DISTINCT
antp_antp AS numero, 
antp_titu AS titulo, 
string_agg(DISTINCT estantp_estd || ' - ' || nombre || ' ' || apellido, ', ') AS autores,
antp_eantp AS estado,
string_agg(DISTINCT acono_nom, ', ') AS tematica
FROM trabajosdegrado.ant_tantp
JOIN trabajosdegrado.ant_testantp ON antp_antp=estantp_antp
JOIN trabajosdegrado.ge_testd ON estantp_estd=estd_estd
JOIN polux_usuario ON estd_us=id_usuario
JOIN trabajosdegrado.ant_tacantp ON acantp_antp=antp_antp
JOIN trabajosdegrado.ge_tacono ON acantp_acono=acono_acono
WHERE 1=1 AND (antp_eantp&lt;&gt;'CANCELADO' AND antp_eantp&lt;&gt;'FINALIZADO' AND antp_eantp&lt;&gt;'PROYECTO' AND antp_eantp&lt;&gt;'NO APROBADO')
GROUP BY antp_antp

UNION

SELECT DISTINCT
proy_proy AS numero, 
proy_titu AS titulo, 
string_agg(DISTINCT estproy_estd || ' - ' || nombre || ' ' || apellido, ', ') AS autores,
proy_eproy AS estado,
string_agg(DISTINCT acono_nom, ', ') AS tematica
FROM trabajosdegrado.pry_tproy
JOIN trabajosdegrado.pry_testpry ON proy_proy=estproy_proy
JOIN trabajosdegrado.ge_testd ON estproy_estd=estd_estd
JOIN polux_usuario ON estd_us=id_usuario
JOIN trabajosdegrado.pry_tacproy ON acproy_proy=proy_proy
JOIN trabajosdegrado.ge_tacono ON acproy_acono=acono_acono
WHERE 1=1
AND (proy_eproy&lt;&gt;'CANCELADO' 
AND proy_eproy&lt;&gt;'FINALIZADO' 
AND proy_eproy&lt;&gt;'INFORME FINAL')
[AND proy_pcur IN ( {proyecto} )]
GROUP BY proy_proy

UNION

SELECT DISTINCT
info_info AS numero, 
info_titu AS titulo, 
string_agg(DISTINCT estinfo_est || ' - ' || nombre || ' ' || apellido, ', ') AS autores,
info_einfo AS estado,
string_agg(DISTINCT acono_nom, ', ') AS tematica
FROM trabajosdegrado.inf_tinfo
JOIN trabajosdegrado.inf_testinfo ON info_info=estinfo_info
JOIN trabajosdegrado.ge_testd ON estinfo_est=estd_estd
JOIN polux_usuario ON estd_us=id_usuario
JOIN trabajosdegrado.inf_tacinfo ON acinfo_info=info_info
JOIN trabajosdegrado.ge_tacono ON acinfo_acono=acono_acono
WHERE 1=1 
AND (info_einfo&lt;&gt;'CANCELADO' 
AND info_einfo&lt;&gt;'FINALIZADO')
[AND info_pcur IN ( {proyecto} )]
GROUP BY info_info

ORDER BY numero

) AS F</SQLRaw></SQL><QueryColumns><QueryColumn><Name>num</Name><TableName></TableName><ColumnName>row_number() over (order by estado, numero) as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF>1cm</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title> </column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>numero</Name><TableName></TableName><ColumnName>numero</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>NÚMERO</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>titulo</Name><TableName></TableName><ColumnName>titulo</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>TÍTULO</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>estado</Name><TableName></TableName><ColumnName>estado</ColumnName><C